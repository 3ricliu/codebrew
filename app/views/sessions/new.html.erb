<h1>Codebrew</h1>
<h2>Sign In!</h2>

<form class="" action="<%= session_url %>" method="post">
  <%= render "/helpers/auth" %>

  <label for="username"> Username
    <input type="text" name="user[username]" value="">
  </label>
  <br>
  <label for="password"> Password
    <input type="text" name="user[password]" value="">
  </label>
  <br>
  <input type="submit" name="name" value="Sign In!">
</form>
